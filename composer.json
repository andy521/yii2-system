{
  "name": "yuncms/yii2-system",
  "description": "The system extension for the yuncms",
  "keywords": [
    "yuncms",
    "system",
    "common"
  ],
  "type": "yii2-extension",
  "license": "MIT",
  "require": {
    "yiisoft/yii2": "~2.0.6",
    "yii2tech/ar-position": "~1.0",
    "2amigos/yii2-qrcode-helper": "~1.0",
    "xutl/yii2-ueditor-widget": "~1.0",
    "pheme/yii2-toggle-column": "*"
  },
  "require-dev": {
    "phpunit/PHPUnit": "~6.0"
  },
  "autoload": {
    "files": [
      "src/helpers.php"
    ],
    "psr-4": {
      "yuncms\\system\\": "src"
    }
  },
  "extra": {
    "bootstrap": "yuncms\\system\\Bootstrap"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
